<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Text?>

<BorderPane fx:id="BP_Okno" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ControllerBlad">
   <top>
      <Text fx:id="T_TypBledu" strokeType="OUTSIDE" strokeWidth="0.0" text="Wprowadzono błędny format daty!" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </BorderPane.margin>
      </Text>
   </top>
   <bottom>
      <Button fx:id="B_Ok" mnemonicParsing="false" onAction="#B_Ok_Action" prefWidth="100.0" text="Ok" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </BorderPane.margin>
      </Button>
   </bottom>
   <center>
      <Text fx:id="T_OpisBledu" strokeType="OUTSIDE" strokeWidth="0.0" text="Prawidłowy format to RRRR:MM:DD" BorderPane.alignment="CENTER" />
   </center>
</BorderPane>
